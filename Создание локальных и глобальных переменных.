#include<iostream>
использование пространства имен std;
интервал а = 100, б = 20;
пустота f1 (целое а)
{
     а += 10;
     cout << "f1:\t" << a << "\t" << ::a << endl;
}
пустота f2 (интервал б)
{
     б* =2;
     cout << "f2\t" << b <<"/t" << a << endl;
}
интервал основной(){
     cout << "main:\t" << a << "\t" << b << endl;
     f1(а); f2(б);
     cout << "main\t" << a << "\t" << b << endl;
     вернуть 0;
}
