#include<iostream>
#include <math.h>
using namespace std;

 int main()
 {
     float x,y;
     cout << "enter number x ";
     cin>>x;
     if(x<0){
         y=(pow(x,3)+1)*pow(x,2);
         cout << "y=" << y << endl;
     }
     else{
         if(x>=1){
            y= abs(pow(x,2)-5*x+1);
            cout<<"y =  |x|= " << y << endl;
         }
     else{
         {
             y=0;
        cout <<"y =" << y << endl;
         }
        
     }
     }
     return 0 ; 
 } 
